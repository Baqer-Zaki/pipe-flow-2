FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    cylinder;
        type    faceSet;
        action  new;
        source  patchToFace;
	source  patchToFace;
	sourceInfo
	        {
		   patch cylinder;
		}
    
       }
       ////////////////////////
       //
       {
         name    bottom_patch_negative;
	 type    faceSet;
         action  new;
	 source  boxToFace;
	 sourceInfo
	       {
	       box (-0.6 -0.6 0.1) (0.6 0.6 41);
	       }
               }
//////////////////////

	 {
	  name    upper_patch_negative;
          type    faceSet;
          action  new;
	  source  boxToFace;
	  sourceInfo
          {
	   box (-0.6 -0.6 -1) (0.6 0.6 39.99);
	  }

           }
////////////////////
// new set bottom patch
 {
	 name    bottomPatch;
	 type    faceSet;
	 action  new;
	 source  faceToFace;
	 sourceInfo
	 {				                                    set cylinder;						   }
 }
{
	     name    bottomPatch;
             type    faceSet;
	     action  delete;
	     source  faceToFace;
	     sourceInfo                                                       {                                                                  set bottom_patch_negative;                                      }
  }
////////////////////  
//set 2 
{
	        name    upperPatch;
	        type    faceSet;
	        action  new;
	        source  faceToFace;
	        sourceInfo						              {                                                                  set cylinder;                                                  }			   
}
{
	        name    upperPatch;
	        type    faceSet;
                action  delete;
	        source  faceToFace;					         sourceInfo                                                       {                                                                  set upper_patch_negative;                                      }
								 }
///////////
//set 3 the wall 
{
              name    wall;
	      type    faceSet;
	      action  new;
	      source  faceToFace;
	      sourceInfo                                                            {                                                                  set cylinder;                                                  }
}


{
	      name    wall;
	      type    faceSet;
	     action  delete;						      source  faceToFace;                                              sourceInfo                                                       {                                                                  set bottomPatch;                                     
       }

}
{
	      name    wall;
	      type    faceSet;
	      action  delete;	
	      source  faceToFace;                                              sourceInfo                                                       {                                                                  set  upperPatch;                                     
	  }

}





);

